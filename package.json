{
  "name": "@neoncity/causefe",
  "version": "0.1.1",
  "description": "The main frontend, where causes are managed and viewed.",
  "author": "NeonCity team",
  "scripts": {
    "build": "tsc && webpack",
    "serve-dev": "tsc --watch & nodemon out/server/index.js",
    "serve-prod": "node out/server/index.js",
    "pretest": "npm run build",
    "test": "npm run build && istanbul cover --config ./.istanbul.yml ./node_modules/mocha/bin/_mocha -- $(find out -name '*-test.js') && remap-istanbul -i ./out/coverage/coverage-final.json -o ./out/coverage/coverage-remapped.json && coverage-filter -i ./out/coverage/coverage-remapped.json -o ./out/coverage/coverage-filtered.json",
    "push-coverage-to-codecov": "codecov -f ./out/coverage/coverage-filtered.json"
  },
  "respository": {
    "type": "git",
    "url": "git+https://github.com/neoncity/causefe.git"
  },
  "license": "UNLICENSED",
  "private": true,
  "homepage": "https://github.com/neoncity/causefe#readme",
  "filesPack": {
    "out/client": "c:client",
    "out/server": "c:server",
    "webpack.config.js": "c:webpack.config.js",
    "package.json": "c:package.json",
    "tsconfig.json": "c:tsconfig.json"
  },
  "dependencies": {
    "es6-promise": "^4.0.5",
    "express": "^4.14.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "redux": "^3.6.0",
    "webpack-dev-middleware": "^1.9.0"
  },
  "devDependencies": {
    "@neoncity/common-js": "^0.0.11",
    "@neoncity/coverage-filter": "^0.0.7",
    "@neoncity/togemfury": "^0.0.9",
    "@types/chai": "^3.4.34",
    "@types/es6-promise": "0.0.32",
    "@types/express": "^4.0.35",
    "@types/mocha": "^2.2.37",
    "@types/node": "^7.0.1",
    "@types/react": "^15.0.2",
    "@types/react-dom": "^0.14.21",
    "@types/react-redux": "^4.4.36",
    "@types/react-router": "^2.0.46",
    "@types/webpack": "^2.2.2",
    "@types/webpack-dev-middleware": "^1.9.1",
    "chai": "^3.5.0",
    "codecov": "^1.0.1",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "istanbul": "^0.4.5",
    "json-loader": "^0.5.4",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "remap-istanbul": "^0.8.4",
    "style-loader": "^0.13.1",
    "ts-loader": "^1.3.3",
    "typescript": "^2.1.5",
    "webpack": "^1.14.0",
    "webpack-fail-plugin": "^1.0.5"
  }
}
