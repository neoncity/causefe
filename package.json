{
  "name": "@neoncity/causefe",
  "version": "2.25.1",
  "description": "The main frontend, where causes are managed and viewed.",
  "author": "NeonCity team",
  "scripts": {
    "build": "tsc && webpack -p",
    "build-analyze": "tsc && webpack -p --json > out/build-stats.json",
    "build-analyze2": "tsc && webpack --profile --progress",
    "serve-dev": "tsc --watch & node out/server/index.js",
    "serve-prod": "node out/server/index.js",
    "pretest": "npm run build",
    "test": "npm run build && istanbul cover --config ./.istanbul.yml ./node_modules/mocha/bin/_mocha -- $(find out -name '*-test.js') && remap-istanbul -i ./out/coverage/coverage-final.json -o ./out/coverage/coverage-remapped.json && coverage-filter -i ./out/coverage/coverage-remapped.json -o ./out/coverage/coverage-filtered.json",
    "push-coverage-to-codecov": "codecov -f ./out/coverage/coverage-filtered.json"
  },
  "respository": {
    "type": "git",
    "url": "git+https://github.com/neoncity/causefe.git"
  },
  "license": "UNLICENSED",
  "private": true,
  "homepage": "https://github.com/neoncity/causefe#readme",
  "filesPack": {
    "out/client": "c:client",
    "out/server": "c:server",
    "webpack.config.js": "c:webpack.config.js",
    "package.json": "c:package.json",
    "tsconfig.json": "c:tsconfig.json"
  },
  "dependencies": {
    "@neoncity/common-js": "~1",
    "@neoncity/common-server-js": "~3",
    "@neoncity/core-sdk-js": "~6",
    "@neoncity/identity-sdk-js": "~4",
    "async-middleware": "=1.0.2",
    "auth0-lock": "=10.11.0",
    "classnames": "=2.2.5",
    "compression": "=1.6.2",
    "continuation-local-storage": "=3.2.0",
    "es6-promise": "=4.0.5",
    "express": "=4.14.0",
    "filepicker-js": "=2.4.18",
    "history": "=3.3.0",
    "http-status-codes": "=1.1.6",
    "isomorphic-fetch": "=2.2.1",
    "moment": "=2.18.1",
    "mustache": "=2.3.0",
    "normalize.css": "=7.0.0",
    "query-string": "=4.3.2",
    "raynor": "=0.8.0",
    "react": "=15.4.2",
    "react-datepicker": "=0.44.0",
    "react-dom": "=15.4.2",
    "react-helmet": "=5.1.3",
    "react-redux": "=5.0.2",
    "react-router": "=3.0.2",
    "redux": "=3.6.0",
    "serialize-javascript": "=1.3.0",
    "webpack-dev-middleware": "=1.9.0"
  },
  "devDependencies": {
    "@neoncity/togemfury": "~0",
    "@types/auth0-js": "git+https://git@github.com/neoncity/auth0-js-with-v7.git",
    "@types/auth0-lock": "=10.9.2",
    "@types/chai": "=3.4.34",
    "@types/classnames": "=2.2.0",
    "@types/compression": "=0.0.33",
    "@types/continuation-local-storage": "=3.2.1",
    "@types/es6-promise": "=0.0.32",
    "@types/express": "=4.0.35",
    "@types/facebook-js-sdk": "=2.8.1",
    "@types/http-status-codes": "=1.0.29",
    "@types/isomorphic-fetch": "=0.0.32",
    "@types/mocha": "=2.2.37",
    "@types/mustache": "=0.8.29",
    "@types/node": "=7.0.1",
